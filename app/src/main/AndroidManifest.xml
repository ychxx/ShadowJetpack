        </activity>
        <activity android:name=".TestShowLoadingActivity"/>
        <activity android:name=".TestSpecialReleaseActivity"/>
        <activity android:name=".TestSpecialReleaseActivity2"/>
        <activity android:name=".TestRefreshActivity"/>
        <activity android:name=".TestWidgetActivity"/>
        <activity android:name=".chart.TestChartLineActivity"/>

    </application>

</manifest>